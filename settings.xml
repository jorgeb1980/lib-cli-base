<!-- Only used for deploying to github packages -->
<settings>
    <profiles>
        <profile>
            <id>github-deploy</id>
            <repositories>
                <repository>
                    <id>github</id>
                    <name>GitHub jorgeb1980 Apache Maven Packages</name>
                    <url>https://maven.pkg.github.com/jorgeb1980/lib-shell-commands</url>
                </repository>
            </repositories>
        </profile>
    </profiles>

    <activeProfiles>
        <activeProfile>github-deploy</activeProfile>
    </activeProfiles>

    <servers>
        <server>
            <id>github</id>
            <username>jorgeb1980</username>
            <password>${env.GITHUB_TOKEN}}</password>
        </server>
    </servers>
</settings>